<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.github.itdachen.admin.mapper.IClazzInfoMapper">


    <!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="com.github.itdachen.admin.sdk.vo.ClazzInfoVO" id="clazzInfoMap">
        <result property="id" column="id"/>
        <result property="tenantId" column="tenant_id"/>
        <result property="deptFuncCode" column="dept_func_code"/>
        <result property="deptFuncTitle" column="dept_func_title"/>
        <result property="thatLevelCode" column="that_level_code"/>
        <result property="thatLevelTitle" column="that_level_title"/>
        <result property="clazzFuncCode" column="clazz_func_code"/>
        <result property="clazzFuncTitle" column="clazz_func_title"/>
        <result property="clazzCode" column="clazz_code"/>
        <result property="clazzTitle" column="clazz_title"/>
        <result property="validFlag" column="valid_flag"/>
        <result property="remarks" column="remarks"/>
    </resultMap>

    <!-- VO 类查询 -->
    <sql id="selectClazzInfoVO">
        SELECT id, tenant_id, dept_func_code, dept_func_title, that_level_code, that_level_title, clazz_func_code, clazz_func_title, clazz_code, clazz_title, valid_flag, remarks
        FROM ta_fly_clazz_info
    </sql>

    <!-- 分页和导出查询参数 -->
    <sql id="selectParamsWhere">
        <where>
            <if test="null != tenantId  and '' != tenantId ">
                AND tenant_id = #{tenantId}
            </if>
            <if test="null != deptFuncCode  and '' != deptFuncCode ">
                AND dept_func_code = #{deptFuncCode}
            </if>
            <if test="null != thatLevelCode  and '' != thatLevelCode ">
                AND that_level_code = #{thatLevelCode}
            </if>
            <if test="null != clazzFuncCode  and '' != clazzFuncCode ">
                AND clazz_func_code = #{clazzFuncCode}
            </if>
            <if test="null != clazzCode  and '' != clazzCode ">
                AND clazz_code LIKE CONCAT(CONCAT('%',#{clazzCode}),'%')
            </if>
            <if test="null != clazzTitle  and '' != clazzTitle ">
                AND clazz_title LIKE CONCAT(CONCAT('%',#{clazzTitle}),'%')
            </if>
            <if test="null != validFlag  and '' != validFlag ">
                AND valid_flag = #{validFlag}
            </if>
            <if test="null != removeFlag  and '' != removeFlag ">
                AND remove_flag = #{removeFlag}
            </if>
        </where>
    </sql>

    <select id="list_COUNT" parameterType="com.github.itdachen.admin.sdk.query.ClazzInfoQuery" resultType="java.lang.Long">
        SELECT COUNT(id)
        FROM  ta_fly_clazz_info
        <include refid="selectParamsWhere"></include>
    </select>

    <!-- 集合查询 -->
    <select id="list" parameterType="com.github.itdachen.admin.sdk.query.ClazzInfoQuery" resultMap="clazzInfoMap">
        <include refid="selectClazzInfoVO"/>
        <include refid="selectParamsWhere"></include>
        ORDER BY id DESC
    </select>

    <!-- 根据主键查询 -->
    <select id="selectClazzInfoVO" parameterType="java.lang.String" resultMap="clazzInfoMap">
        <include refid="selectClazzInfoVO"/>
        WHERE id = #{id}
    </select>

    <!-- 批量添加 -->
    <insert id="batchSave" parameterType="java.util.List">
        INSERT INTO ta_fly_clazz_info (id ,
        tenant_id ,
        dept_func_code ,
        dept_func_title ,
        that_level_code ,
        that_level_title ,
        clazz_func_code ,
        clazz_func_title ,
        clazz_code ,
        clazz_title ,
        valid_flag ,
        remarks ,
        remove_flag ,
        create_time ,
        create_user ,
        create_user_id ,
        update_time ,
        update_user ,
        update_user_id
        ) VALUES
        <foreach collection="list" item="item" separator=",">
            (#{item.id},
            #{item.tenantId},
            #{item.deptFuncCode},
            #{item.deptFuncTitle},
            #{item.thatLevelCode},
            #{item.thatLevelTitle},
            #{item.clazzFuncCode},
            #{item.clazzFuncTitle},
            #{item.clazzCode},
            #{item.clazzTitle},
            #{item.validFlag},
            #{item.remarks},
            #{item.removeFlag},
            #{item.createTime},
            #{item.createUser},
            #{item.createUserId},
            #{item.updateTime},
            #{item.updateUser},
            #{item.updateUserId})
        </foreach>
    </insert>

    <!-- 批量更新 -->
    <update id="batchUpdate" parameterType="java.util.List">
        <foreach collection="list" item="item" index="index" separator=";">
            UPDATE ta_fly_clazz_info
            <set>
                <if test="null != item.id  and '' != item.id ">id =
                    #{item.id},
                </if>
                <if test="null != item.tenantId  and '' != item.tenantId ">tenant_id =
                    #{item.tenantId},
                </if>
                <if test="null != item.deptFuncCode  and '' != item.deptFuncCode ">dept_func_code =
                    #{item.deptFuncCode},
                </if>
                <if test="null != item.deptFuncTitle  and '' != item.deptFuncTitle ">dept_func_title =
                    #{item.deptFuncTitle},
                </if>
                <if test="null != item.thatLevelCode  and '' != item.thatLevelCode ">that_level_code =
                    #{item.thatLevelCode},
                </if>
                <if test="null != item.thatLevelTitle  and '' != item.thatLevelTitle ">that_level_title =
                    #{item.thatLevelTitle},
                </if>
                <if test="null != item.clazzFuncCode  and '' != item.clazzFuncCode ">clazz_func_code =
                    #{item.clazzFuncCode},
                </if>
                <if test="null != item.clazzFuncTitle  and '' != item.clazzFuncTitle ">clazz_func_title =
                    #{item.clazzFuncTitle},
                </if>
                <if test="null != item.clazzCode  and '' != item.clazzCode ">clazz_code =
                    #{item.clazzCode},
                </if>
                <if test="null != item.clazzTitle  and '' != item.clazzTitle ">clazz_title =
                    #{item.clazzTitle},
                </if>
                <if test="null != item.validFlag  and '' != item.validFlag ">valid_flag =
                    #{item.validFlag},
                </if>
                <if test="null != item.remarks  and '' != item.remarks ">remarks =
                    #{item.remarks},
                </if>
                <if test="null != item.removeFlag  and '' != item.removeFlag ">remove_flag =
                    #{item.removeFlag},
                </if>
                <if test="null != item.createTime ">create_time =
                    #{item.createTime},
                </if>
                <if test="null != item.createUser  and '' != item.createUser ">create_user =
                    #{item.createUser},
                </if>
                <if test="null != item.createUserId  and '' != item.createUserId ">create_user_id =
                    #{item.createUserId},
                </if>
                <if test="null != item.updateTime ">update_time =
                    #{item.updateTime},
                </if>
                <if test="null != item.updateUser  and '' != item.updateUser ">update_user =
                    #{item.updateUser},
                </if>
                <if test="null != item.updateUserId  and '' != item.updateUserId ">update_user_id =
                    #{item.updateUserId},
                </if>
            </set>
            WHERE id = #{item.id}
        </foreach>
    </update>




</mapper>